<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <include
        android:id="@+id/include1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        layout="@layout/layout_title" >
    </include>

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_weight="1" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/linearLayout1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <LinearLayout
                    android:id="@+id/linearLayout2"
                    android:layout_width="fill_parent"
                    android:layout_height="80dp" >

                    <FrameLayout
                        android:id="@+id/FrameLayout03"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:padding="3dp" >

                        <LinearLayout
                            android:id="@+id/linearLayoutIcon1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/icon_background_drawable"
                            android:clickable="true"
                            android:orientation="vertical"
                            android:padding="2dp" >

                            <ImageView
                                android:id="@+id/imageViewIcon1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:scaleType="centerCrop"
                                android:src="@drawable/bus" />

                            <TextView
                                android:id="@+id/textViewTitle1"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_horizontal"
                                android:text="Bus"
                                android:textColor="#ffffff"
                                android:textSize="10dp" />
                        </LinearLayout>
                    </FrameLayout>

                    <FrameLayout
                        android:id="@+id/FrameLayout02"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:padding="3dp" >

                        <LinearLayout
                            android:id="@+id/linearLayoutIcon2"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/icon_background_drawable"
                            android:clickable="true"
                            android:orientation="vertical"
                            android:padding="2dp" >

                            <ImageView
                                android:id="@+id/imageViewIcon2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:scaleType="centerCrop"
                                android:src="@drawable/taxi" />

                            <TextView
                                android:id="@+id/textViewTitle2"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_horizontal"
                                android:text="Taxi"
                                android:textColor="#ffffff"
                                android:textSize="10dp" />
                        </LinearLayout>
                    </FrameLayout>

                    <FrameLayout
                        android:id="@+id/FrameLayout01"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:padding="3dp" >

                        <LinearLayout
                            android:id="@+id/linearLayoutIcon3"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/icon_background_drawable"
                            android:clickable="true"
                            android:orientation="vertical"
                            android:padding="2dp" >

                            <ImageView
                                android:id="@+id/imageViewIcon3"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:scaleType="centerCrop"
                                android:src="@drawable/rentacar" />

                            <TextView
                                android:id="@+id/textViewTitle3"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_horizontal"
                                android:text="Rent a car"
                                android:textColor="#ffffff"
                                android:textSize="10dp" />
                        </LinearLayout>
                    </FrameLayout>

                    <FrameLayout
                        android:id="@+id/frameLayout1"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:padding="3dp" >

                        <LinearLayout
                            android:id="@+id/linearLayoutIcon4"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/icon_background_drawable"
                            android:clickable="true"
                            android:orientation="vertical"
                            android:padding="2dp" >

                            <ImageView
                                android:id="@+id/imageViewIcon4"
                                android:layout_width="wrap_content"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"
                                android:scaleType="centerCrop"
                                android:src="@drawable/rentascooter" />

                            <TextView
                                android:id="@+id/textViewTitle4"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_horizontal"
                                android:text="Hosteli"
                                android:textColor="#ffffff"
                                android:textSize="10dp" />
                        </LinearLayout>
                    </FrameLayout>
                </LinearLayout>

                <FrameLayout
                    android:id="@+id/FrameLayout1"
                    android:layout_width="fill_parent"
                    android:layout_height="240dp"
                    android:padding="3dp" >

                    <ProgressBar
                        android:id="@+id/progressBar1"
                        style="?android:attr/progressBarStyleSmall"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center" />

                    <ImageView
                        android:id="@+id/imageViewBigBaner1"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:scaleType="centerCrop" />
                </FrameLayout>

                <LinearLayout
                    android:id="@+id/linearLayout4"
                    android:layout_width="fill_parent"
                    android:layout_height="80dp"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:id="@+id/linearLayout7"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:id="@+id/LinearLayout07"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:orientation="horizontal" >

                            <FrameLayout
                                android:id="@+id/FrameLayout04"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"
                                android:padding="3dp" >

                                <LinearLayout
                                    android:id="@+id/linearLayoutIcon5"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:background="@drawable/icon_background_drawable"
                                    android:clickable="true"
                                    android:orientation="vertical"
                                    android:padding="2dp" >

                                    <ImageView
                                        android:id="@+id/imageViewIcon5"
                                        android:layout_width="fill_parent"
                                        android:layout_height="fill_parent"
                                        android:layout_weight="1"
                                        android:scaleType="centerCrop"
                                        android:src="@drawable/avio" />

                                    <TextView
                                        android:id="@+id/textViewTitle5"
                                        android:layout_width="fill_parent"
                                        android:layout_height="wrap_content"
                                        android:gravity="center_horizontal"
                                        android:text="Sobe"
                                        android:textColor="#ffffff"
                                        android:textSize="10dp" />
                                </LinearLayout>
                            </FrameLayout>

                            <FrameLayout
                                android:id="@+id/FrameLayout05"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"
                                android:padding="3dp" >

                                <LinearLayout
                                    android:id="@+id/linearLayoutIcon6"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:background="@drawable/icon_background_drawable"
                                    android:clickable="true"
                                    android:orientation="vertical"
                                    android:padding="2dp" >

                                    <ImageView
                                        android:id="@+id/ImageView01"
                                        android:layout_width="fill_parent"
                                        android:layout_height="fill_parent"
                                        android:layout_weight="1"
                                        android:scaleType="centerCrop"
                                        android:src="@drawable/voz" />

                                    <TextView
                                        android:id="@+id/textViewTitle6"
                                        android:layout_width="fill_parent"
                                        android:layout_height="wrap_content"
                                        android:gravity="center_horizontal"
                                        android:text="Kampovi"
                                        android:textColor="#ffffff"
                                        android:textSize="10dp" />
                                </LinearLayout>
                            </FrameLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/linearLayout6"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:orientation="horizontal" >

                            <FrameLayout
                                android:id="@+id/frameLayout2"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"
                                android:padding="3dp" >

                                <LinearLayout
                                    android:id="@+id/linearLayoutIcon7"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:background="@drawable/icon_background_drawable"
                                    android:clickable="true"
                                    android:orientation="vertical"
                                    android:padding="2dp" >

                                    <ImageView
                                        android:id="@+id/imageViewIcon7"
                                        android:layout_width="fill_parent"
                                        android:layout_height="fill_parent"
                                        android:layout_weight="1"
                                        android:scaleType="centerCrop"
                                        android:src="@drawable/rent_a_boat" />

                                    <TextView
                                        android:id="@+id/textViewTitle7"
                                        android:layout_width="fill_parent"
                                        android:layout_height="wrap_content"
                                        android:gravity="center_horizontal"
                                        android:text="Rent a local"
                                        android:textColor="#ffffff"
                                        android:textSize="10dp" />
                                </LinearLayout>
                            </FrameLayout>

                            <FrameLayout
                                android:id="@+id/frameLayout3"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"
                                android:padding="3dp" >
                            </FrameLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

    <LinearLayout
        android:id="@+id/linearLayoutGoogleBaner"
        android:layout_width="fill_parent"
        android:layout_height="70dp"
        android:orientation="vertical" >
    </LinearLayout>

</LinearLayout>